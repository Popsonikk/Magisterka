            </rowConstraints>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Podaj ufność" textAlignment="CENTER"
                  wrappingWidth="150.0">
                <font>
                    <Font size="18.0"/>
                </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Podaj lift" textAlignment="CENTER" wrappingWidth="150.0"
